{{>layouts/header}}

<table class="table">
    <thead>

    <tr>
        <th scope="col">Id</th>
        <th scope="col">Title</th>
        <th scope="col">Content</th>
    </tr>
    </thead>
    <tbody>
    {{#article}} <!--모델에 등록한 article은 뷰 페이지에서 mustache문법인 이중줄괄호를 이용해 출력한다.-->
    <tr>
        <th>{{id}}</th>
        <td>{{title}}</td>
        <td>{{content}}</td>
    </tr>
    {{/article}}
    </tbody>
</table>

<a href="/articles">Go to Article List</a>

{{>layouts/footer}}